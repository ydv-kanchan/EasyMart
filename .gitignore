# Ignore node_modules in all subfolders
node_modules/

# Ignore environment files (security risk to push these)
.env
.env.local
.env.production
.env.development

# Ignore dependency lock files (optional, but useful if you don't want them in Git)
package-lock.json
pnpm-lock.yaml
yarn.lock

# Ignore Vite cache files (since you're using Vite in frontend)
.vite/

# Ignore build and output folders
dist/
out/
build/

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE / Editor settings (optional)
.vscode/
.idea/
*.swp

# Ignore Frontend & Backend-specific files
Frontend/node_modules/
Frontend/.gitignore
Backend/node_modules/
Backend/.gitignore
